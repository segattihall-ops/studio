-{"version":3,"sources":["../src/index.tsx"],"sourcesContent":["import React, { createContext, useContext, useEffect, useMemo, useRef, useState, useLayoutEffect, useCallback, forwardRef, ComponentProps, Ref, ReactNode } from 'react';\nimport * as DialogPrimitive from '@radix-ui/react-dialog';\nimport { RemoveScroll } from 'react-remove-scroll';\n\n// TODO: Use `react-remove-scroll-bar` instead of `body-scroll-lock`\n\nconst VaulContext = createContext<VaulContextValue | undefined>(undefined);\n\ninterface VaulContextValue {\n  isDragging: boolean;\n  isClosing: boolean;\n  drawerRef: React.RefObject<HTMLDivElement>;\n  handlePointerDown: (e: React.PointerEvent<HTMLDivElement>) => void;\n  handlePointerMove: (e: React.PointerEvent<HTMLDivElement>) => void;\n  handlePointerUp: (e: React.PointerEvent<HTMLDivElement>) => void;\n  handleDrag: (e: React.PointerEvent<HTMLDivElement>) => void;\n  handleRelease: (e: React.PointerEvent<HTMLDivElement>) => void;\n  closeDrawer: () => void;\n  snapPoints: (number | string)[] | undefined;\n  activeSnapPoint: number | string | null;\n  setActiveSnapPoint: (snapPoint: number | string | null) => void;\n  visible: boolean;\n  drawerHeightRef: React.RefObject<number>;\n}\n\nexport const useVaul = () => {\n  const context = useContext(VaulContext);\n\n  if (!context) {\n    throw new Error('useVaul must be used within a <Vaul.Root />');\n  }\n\n  return context;\n};\n\nconst stopListening = (cb: () => void) => {\n  document.removeEventListener('pointermove', cb);\n  document.removeEventListener('pointerup', cb);\n  document.removeEventListener('pointercancel', cb);\n};\n\nfunction getTranslateY(element: HTMLDivElement): number | null {\n  const style = window.getComputedStyle(element);\n  const transform = style.getPropertyValue('transform');\n\n  if (transform === 'none') return null;\n\n  const matrix = new DOMMatrixReadOnly(transform);\n  return matrix.m42;\n}\n\nfunction set(el: HTMLElement | undefined, newTranslate: number) {\n  if (!el) return;\n  el.style.transform = `translateY(${newTranslate}px)`;\n}\n\ninterface RootProps {\n  open?: boolean;\n  onOpenChange?: (open: boolean) => void;\n  children: ReactNode;\n  shouldScaleBackground?: boolean;\n}\n\nconst Root: React.FC<RootProps> = ({ children, open, onOpenChange, shouldScaleBackground = true }) => {\n  const [isOpen, setIsOpen] = useState(open || false);\n  const hasBeenOpened = useRef(false);\n  const isClosing = useRef(false);\n  const isDragging = useRef(false);\n  const [snapPoints, setSnapPoints] = useState<(string | number)[] | undefined>(undefined);\n  const [activeSnapPoint, setActiveSnapPoint] = useState<string | number | null>(null);\n  const drawerRef = useRef<HTMLDivElement>(null);\n  const overlayRef = useRef<HTMLDivElement>(null);\n  const [visible, setVisible] = useState(false);\n  const drawerHeightRef = useRef(0);\n\n  useEffect(() => {\n    if (open) {\n      hasBeenOpened.current = true;\n    }\n\n    if (onOpenChange) {\n      if (open !== isOpen) {\n        setIsOpen(open);\n      }\n    }\n  }, [open, isOpen, onOpenChange]);\n\n  // Need to get the reference of the drawer as it's not a children of the context provider\n\n  function closeDrawer() {\n    if (isClosing.current) return;\n    isClosing.current = true;\n\n    onOpenChange?.(false);\n    setIsOpen(false);\n\n    if (drawerRef.current) {\n      drawerRef.current.style.transform = `translateY(${drawerHeightRef.current}px)`;\n      drawerRef.current.style.transition = `transform 0.5s cubic-bezier(0.32, 0.72, 0, 1)$`;\n    }\n\n    // Re-enable body scroll\n  }\n\n  const onAnimationEnd = () => {\n    if (isClosing.current) {\n      setVisible(false);\n      isClosing.current = false;\n    }\n  };\n\n  useLayoutEffect(() => {\n    if (isOpen) {\n      setVisible(true);\n    } else {\n      hasBeenOpened.current = false;\n    }\n  }, [isOpen]);\n\n  const value = useMemo(\n    () => ({\n      isDragging: isDragging.current,\n      isClosing: isClosing.current,\n      drawerRef,\n      handlePointerDown,\n      handlePointerMove,\n      handlePointerUp,\n      handleDrag,\n      handleRelease,\n      closeDrawer,\n      snapPoints: snapPoints,\n      activeSnapPoint,\n      setActiveSnapPoint,\n      visible,\n      drawerHeightRef,\n    }),\n    [isDragging, isClosing, snapPoints, activeSnapPoint, visible],\n  );\n\n  function onDrag(event: React.PointerEvent<HTMLDivElement>) {\n    if (!drawerRef.current) return;\n    event.preventDefault();\n\n    const newTranslate = getTranslateY(drawerRef.current) + event.movementY;\n\n    if (newTranslate > 0) {\n      drawerRef.current.style.transform = `translateY(${newTranslate}px)`;\n    }\n  }\n\n  function onRelease(event: React.PointerEvent<HTMLDivElement>) {\n    if (!drawerRef.current) return;\n\n    stopListening(() => onRelease(event));\n\n    if (activeSnapPoint && snapPoints) {\n      const activeSnapPointIndex = snapPoints.findIndex((snapPoint) => snapPoint === activeSnapPoint);\n      const isLastSnapPoint = activeSnapPointIndex === snapPoints.length - 1;\n      const isFirstSnapPoint = activeSnapPointIndex === 0;\n\n      if (isLastSnapPoint) {\n        // It's the last snap point, we shouldn't be able to drag down further\n        if (event.movementY > 0) {\n          return;\n        }\n      } else if (isFirstSnapPoint) {\n        if (event.movementY < 0) {\n          return;\n        }\n      }\n    }\n\n    // Clicking on the overlay should close the drawer\n    const target = event.target as HTMLElement;\n\n    if (target.classList.contains('vaul-overlay')) {\n      closeDrawer();\n      return;\n    }\n\n    isDragging.current = false;\n    const newTranslate = getTranslateY(drawerRef.current);\n\n    if (newTranslate === null) return;\n\n    if (snapPoints && snapPoints.length > 0) {\n      // Find the closest snap point\n      let newY;\n      let closestSnapPoint;\n      const snapPointsInPx = snapPoints.map((snapPoint) => {\n        const isPx = typeof snapPoint === 'string';\n        if (isPx) {\n          return Number(snapPoint.replace('px', ''));\n        } else {\n          return drawerHeightRef.current - snapPoint;\n        }\n      });\n\n      const newSnapPoints = [drawerHeightRef.current, ...snapPointsInPx];\n\n      let closestDistance = Infinity;\n\n      for (let i = 0; i < newSnapPoints.length; i++) {\n        const distance = Math.abs(newTranslate - newSnapPoints[i]);\n        if (distance < closestDistance) {\n          closestDistance = distance;\n          newY = newSnapPoints[i];\n\n          if (i === 0) {\n            closestSnapPoint = null;\n          } else {\n            closestSnapPoint = snapPoints[i - 1];\n          }\n        }\n      }\n\n      drawerRef.current.style.transition = `transform 0.5s cubic-bezier(0.32, 0.72, 0, 1)$`;\n\n      if (newY === drawerHeightRef.current) {\n        closeDrawer();\n        return;\n      }\n\n      if (newY !== undefined) {\n        set(drawerRef.current, newY);\n        setActiveSnapPoint(closestSnapPoint);\n      }\n    } else {\n      // It's not a snap point drawer, we should close it if the user drags down\n      if (newTranslate > drawerHeightRef.current * 0.4) {\n        closeDrawer();\n      } else {\n        drawerRef.current.style.transition = `transform 0.5s cubic-bezier(0.32, 0.72, 0, 1)$`;\n        set(drawerRef.current, 0);\n      }\n    }\n  }\n\n  function onPointerDown(event: React.PointerEvent<HTMLDivElement>) {\n    // Don't do any dragging logic if the user is scrolling\n    const target = event.target as HTMLElement;\n\n    if (isScrollable(target)) {\n      return;\n    }\n\n    if (drawerRef.current) {\n      drawerRef.current.style.transition = 'none';\n    }\n    isDragging.current = true;\n    document.addEventListener('pointermove', onDrag);\n    document.addEventListener('pointerup', onRelease);\n    document.addEventListener('pointercancel', onRelease);\n  }\n\n  const handlePointerDown = useCallback(onPointerDown, [snapPoints, activeSnapPoint, onRelease, onDrag]);\n  const handlePointerMove = useCallback(onDrag, [snapPoints, activeSnapPoint, onRelease, onDrag]);\n  const handlePointerUp = useCallback(onRelease, [snapPoints, activeSnapPoint, onRelease, onDrag]);\n  const handleDrag = useCallback(onDrag, [snapPoints, activeSnapPoint, onRelease, onDrag]);\n  const handleRelease = useCallback(onRelease, [snapPoints, activeSnapPoint, onRelease, onDrag]);\n\n  // We need to have a way to calculate the drawer height, as it's not available in the context\n  // and we don't want to pass it down as a prop.\n  useLayoutEffect(() => {\n    if (!drawerRef.current) return;\n    const height = drawerRef.current.getBoundingClientRect().height;\n\n    if (height > 0) {\n      drawerRef.current.style.transform = `translateY(${height}px)`;\n      drawerHeightRef.current = height;\n    }\n  }, [isOpen]);\n\n  const contextValue = useMemo<VaulContextValue>(\n    () => ({\n      isDragging: isDragging.current,\n      isClosing: isClosing.current,\n      drawerRef,\n      handlePointerDown,\n      handlePointerMove,\n      handlePointerUp,\n      handleDrag,\n      handleRelease,\n      closeDrawer,\n      snapPoints: snapPoints,\n      activeSnapPoint,\n      setActiveSnapPoint,\n      visible,\n      drawerHeightRef,\n    }),\n    [isDragging, isClosing, snapPoints, activeSnapPoint, visible, handlePointerDown, handlePointerMove, handlePointerUp, handleDrag, handleRelease, closeDrawer, setActiveSnapPoint, drawerHeightRef],\n  );\n\n  return (\n    <VaulContext.Provider value={value}>\n      <DialogPrimitive.Root\n        open={isOpen}\n        onOpenChange={(o) => {\n          onOpenChange?.(o);\n          setIsOpen(o);\n        }}\n      >\n        {shouldScaleBackground && (\n          <div\n            className=\"vaul-overlay\"\n            style={{\n              transform: isOpen\n                ? `scale(${1 - (5 * drawerHeightRef.current) / (drawerHeightRef.current * 100)})`\n                : 'none',\n              transition: 'transform 0.5s cubic-bezier(0.32, 0.72, 0, 1)',\n              background: isOpen ? 'hsl(var(--background))' : 'none',\n            }}\n          />\n        )}\n\n        <DialogPrimitive.Portal>\n          <DialogPrimitive.Overlay\n            ref={overlayRef}\n            onClick={closeDrawer}\n            className=\"fixed inset-0 bg-black/40 vaul-overlay\"\n            style={{\n              opacity: isOpen ? 1 : 0,\n              transition: `opacity 0.5s cubic-bezier(0.32, 0.72, 0, 1)`,\n            }}\n          />\n          <VaulContext.Provider\n            value={{\n              ...contextValue,\n              snapPoints,\n              activeSnapPoint,\n              setActiveSnapPoint: (snapPoint) => {\n                setSnapPoints(snapPoints);\n                setActiveSnapPoint(snapPoint);\n              },\n            }}\n          >\n            <DialogPrimitive.Content\n              onAnimationEnd={onAnimationEnd}\n              style={{\n                opacity: isOpen || isClosing.current ? 1 : 0,\n                transition: `opacity 0.5s cubic-bezier(0.32, 0.72, 0, 1)`,\n              }}\n              className=\"fixed inset-0\"\n            >\n              {children}\n            </DialogPrimitive.Content>\n          </VaulContext.Provider>\n        </DialogPrimitive.Portal>\n      </DialogPrimitive.Root>\n    </VaulContext.Provider>\n  );\n};\n\nRoot.displayName = 'Vaul.Root';\n\ninterface ContentProps extends ComponentProps<typeof DialogPrimitive.Content> {\n  children: ReactNode;\n}\n\nfunction isScrollable(el: HTMLElement): boolean {\n  // Element does not exist, so it's not scrollable\n  if (!el) return false;\n\n  const isScrollableY = el.scrollHeight > el.clientHeight;\n\n  // Check if the element has a parent and if it's scrollable\n  if (el.parentElement && isScrollable(el.parentElement)) {\n    return true;\n  }\n\n  return isScrollableY;\n}\n\nconst isReact18 = React.version.startsWith('18');\n\nconst rAF = (v: () => void) => requestAnimationFrame(v);\n\nconst Content: React.ForwardRefExoticComponent<ContentProps> = forwardRef(\n  ({ children, ...rest }, ref) => {\n    const { isDragging, drawerRef, handlePointerDown, handlePointerMove, handlePointerUp, visible, isClosing, drawerHeightRef } = useVaul();\n\n    return (\n      <div\n        ref={drawerRef}\n        onPointerDown={handlePointerDown}\n        onPointerMove={handlePointerMove}\n        onPointerUp={handlePointerUp}\n        style={{\n          backfaceVisibility: 'hidden', // Otherwise it can flicker on mobile\n          pointerEvents: isDragging ? 'none' : 'auto',\n          touchAction: 'none',\n          willChange: 'transform',\n        }}\n        className=\"absolute bottom-0 left-0 w-full bg-white rounded-t-lg [vaul-drawer-visible='true']:visible invisible\"\n        data-vaul-drawer-visible={visible}\n      >\n        {children}\n      </div>\n    );\n  },\n);\n\nContent.displayName = 'Vaul.Content';\n\nconst Drawer = Object.assign(Root, {\n  Content,\n  Trigger: DialogPrimitive.Trigger,\n  Close: DialogPrimitive.Close,\n  Title: DialogPrimitive.Title,\n  Description: DialogPrimitive.Description,\n  Overlay: DialogPrimitive.Overlay,\n  Portal: DialogPrimitive.Portal,\n});\n\nexport { Drawer, useVaul as useDrawer, VaulContext };\n"],"names":["React","createContext","useContext","useState","useRef","useLayoutEffect","useEffect","useMemo","useCallback","forwardRef","DialogPrimitive","RemoveScroll","VaulContext","undefined","stopListening","cb","document","removeEventListener","getTranslateY","element","style","window","getComputedStyle","transform","getPropertyValue","matrix","DOMMatrixReadOnly","m42","set","el","newTranslate","Root","children","open","onOpenChange","shouldScaleBackground","setIsOpen","hasBeenOpened","current","isClosing","isDragging","snapPoints","setSnapPoints","activeSnapPoint","setActiveSnapPoint","drawerRef","overlayRef","visible","setVisible","drawerHeight